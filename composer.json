{
    "name": "pixelfederation/z-engine",
    "description": "Library that provides direct access to native PHP structures",
    "type": "library",
    "require": {
        "php": "^8.0",
        "ext-ffi": "*"
    },
    "license": [
        "MIT"
    ],
    "authors": [
        {
            "name": "Alexander Lisachenko",
            "email": "lisachenko.it@gmail.com"
        }
    ],
    "autoload": {
        "psr-4" : {
            "ZEngine\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4" : {
            "ZEngine\\" : "tests/"
        }
    },
    "minimum-stability": "stable",
    "require-dev": {
        "phpunit/phpunit": "^8.5.15 || ^9.0.0"
    }
}
